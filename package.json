{
  "name": "express-api-ts",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "nodemon src/index.ts",
    "build": "tsc",
    "serve": "node dist/index.js",
    "lint": "eslint --ext .ts src",
    "prettier":"prettier --write \"src/**/*.ts\"",
    "typeorm": "ts-node ./node_modules/typeorm/cli -d typeorm.config.ts",
    "migration:generate": "npm run typeorm migration:generate -d src/database/migrations/$npm_config_name",
    "migration:create": "cd src/database/migrations && npx typeorm-ts-node-commonjs migration:create $npm_config_name",
    "migration:run": "npm run typeorm migration:run",
    "migration:revert": "npm run typeorm migration:revert"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dgmmarin/express-api-ts.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/dgmmarin/express-api-ts/issues"
  },
  "homepage": "https://github.com/dgmmarin/express-api-ts#readme",
  "devDependencies": {
    "@types/bull": "^4.10.0",
    "@types/cors": "^2.8.15",
    "@types/express": "^4.17.20",
    "@types/node": "^20.8.10",
    "@typescript-eslint/eslint-plugin": "^6.11.0",
    "@typescript-eslint/parser": "^6.11.0",
    "eslint": "^8.53.0",
    "express": "^4.18.2",
    "nodemon": "^3.0.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2"
  },
  "dependencies": {
    "@bull-board/express": "^5.9.1",
    "@types/bcrypt": "^5.0.1",
    "@types/express-validator": "^3.0.0",
    "@types/jsonwebtoken": "^9.0.4",
    "@types/node": "^16.11.10",
    "bcrypt": "^5.1.1",
    "bullmq": "^4.13.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express-validator": "^7.0.1",
    "jsonwebtoken": "^9.0.2",
    "koa-swagger-decorator": "^1.8.7",
    "mysql": "^2.18.1",
    "prettier": "^3.1.0",
    "reflect-metadata": "^0.1.13",
    "snakecase-keys": "^5.5.0",
    "swagger-ui-express": "^5.0.0",
    "typeorm": "^0.3.17",
    "typescript": "4.5.2",
    "uuid": "^9.0.1",
    "uuidv4": "^6.2.13"
  }
}
